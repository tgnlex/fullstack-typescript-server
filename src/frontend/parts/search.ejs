<search class="search-bar">
  <form class="search-form">  
    <label class="search-label" for="query">Search Here</label>
    <input 
      type="search" 
      id="query"
      name="query"
      class="form-control" 
      required />
      <button class="search-btn" type="submit">Search!</button>
  </form>
  <section>
    <h3>Results</h3>
    <ul id="results" class="list search-list conditional"></ul>
    <output id="no-results">
  </section>
</search> 
